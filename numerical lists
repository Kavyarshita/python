for value in range(1,5):                                #printing values using range() function
    print(value)

    
numbers=list(range(0,10))                               #using range() function to make a list of numbers 
print(numbers)


odd_numbers=list(range(1,21,2))                         #giving condition in range() function to make a list of numbers
print(odd_numbers)


squares=[value**2 for value in range(0,5)]              #giving condition and range() function inside the list 
print(squares)


multiples_of_5=[]                                       #using append and range() functions to make a list of numbers 
for value in range(0,10):
    multiple_of_5=value*5
    multiples_of_5.append(multiple_of_5)

print(multiples_of_5)
