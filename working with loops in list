games=['basket ball','volley ball','badminton','table tennis','foot ball','chess','snakeandladder']
indoor_games=['chess','snakeandladder']

for game in games:
    if game =='badminton':                                              #using for and if changings case
        print(game.upper())
    else:
        print(game)

        
for indoor_game in indoor_games:
    if indoor_game not in games:                                        #checking whether a value is not in a list
        print(indoor_games+"is very easy than games")
